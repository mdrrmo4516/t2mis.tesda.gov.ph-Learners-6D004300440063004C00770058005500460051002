{
  "name": "profile-viewer-app",
  "version": "1.0.0",
  "private": true,
  "description": "Profile viewer single page application",
  "scripts": {
    "install": "cd frontend && yarn install",
    "build": "cd frontend && GENERATE_SOURCEMAP=false yarn build",
    "start": "cd frontend && yarn start",
    "clean": "rm -rf frontend/build frontend/node_modules/.cache",
    "build:prod": "yarn clean && cd frontend && GENERATE_SOURCEMAP=false yarn build",
    "serve": "cd frontend/build && python3 -m http.server 5000"
  }
}
